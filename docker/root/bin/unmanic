#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'unmanic','console_scripts','unmanic'
__requires__ = 'unmanic'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('unmanic', 'console_scripts', 'unmanic')()
    )
